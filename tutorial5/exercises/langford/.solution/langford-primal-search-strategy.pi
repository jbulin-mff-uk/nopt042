import cp.

main([N]) =>
    N := N.to_int, 
    langford_primal(N, Sequence),
    print("Good search strategy:"),
    time2(solve([inout,up],Sequence)),
    println(Sequence).    

langford_primal(N, Sequence) =>
    Sequence = new_array(2*N),
    Sequence :: 1..N,
    foreach(D in 1..N)
        element(I1, Sequence, D),
        element(I2, Sequence, D),
        I2 #= I1 + D + 1
    end.



